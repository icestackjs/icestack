{
  "name": "@icestack/ui",
  "version": "0.0.0-alpha.2",
  "description": "❤️ IceStack, Web UI for Mobile and PC , open-source Headless component library generator",
  "license": "MIT",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./tailwindcss": {
      "types": "./dist/tailwindcss.d.ts",
      "import": "./dist/tailwindcss.mjs",
      "require": "./dist/tailwindcss.cjs"
    },
    "./unocss": {
      "types": "./dist/unocss.d.ts",
      "import": "./dist/unocss.mjs",
      "require": "./dist/unocss.cjs"
    },
    "./scss/*": "./assets/scss/*",
    "./css/*": "./assets/css/*",
    "./css-resolved/*": "./assets/css-resolved/*",
    "./js/*": "./assets/js/*",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "assets"
  ],
  "bin": {
    "icestack": "bin/icestack.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:base && npm run build:utilities && npm run build:components && npm run build:plugin",
    "build:plugin": "unbuild",
    "_build:plugin": "unbuild",
    "clean": "del assets/{css,js,plugins}",
    "dev": "unbuild --watch",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:dev": "vitest",
    "build:scss": "ts-node --esm scripts/build-scss.ts",
    "build:base": "ts-node --esm scripts/build-scss.ts base",
    "build:utilities": "ts-node --esm scripts/build-scss.ts utilities",
    "build:components": "ts-node --esm scripts/build-scss.ts components",
    "get-coms": "ts-node --esm scripts/get-coms.ts",
    "tailwind-config-viewer": "tailwind-config-viewer -o"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@babel/generator": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@ctrl/tinycolor": "^4.0.2",
    "@icestack/css2js": "workspace:^",
    "c12": "^1.5.1",
    "cac": "^6.7.14",
    "compose-tailwindcss-plugins": "workspace:^",
    "css-selector-tokenizer": "^0.8.0",
    "css-to-tailwindcss-plugin": "workspace:^",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.31",
    "postcss-custom-property-prefixer": "workspace:^",
    "postcss-js": "^4.0.1",
    "postcss-selector-parser": "^6.0.13",
    "rtlcss": "^4.1.1",
    "sass": "^1.69.4",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/css-selector-tokenizer": "^0.7.3",
    "@types/lodash": "^4.14.200",
    "@types/postcss-js": "^4.0.3",
    "@types/rtlcss": "^3.5.3",
    "@unocss/core": "^0.56.5",
    "fast-glob": "^3.3.1",
    "merge": "^2.1.1",
    "sass-true": "^7.0.0",
    "unocss": "^0.56.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}