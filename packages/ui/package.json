{
  "name": "@icestack/ui",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./scss/*": "./assets/scss/*",
    "./css/*": "./assets/css/*",
    "./js/*": "./assets/js/*",
    "./plugins/*": "./assets/plugins/*",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "npm run build:base && npm run build-scss && unbuild",
    "dev": "unbuild --watch",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:dev": "vitest",
    "build:scss": "ts-node --esm scripts/build-scss.ts",
    "build:base": "ts-node --esm scripts/build-scss.ts base",
    "raw": "ts-node --esm scripts/colors.ts",
    "tailwind-config-viewer": "tailwind-config-viewer -o"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@icestack/css2js": "workspace:^",
    "compose-tailwindcss-plugins": "workspace:^",
    "css-to-tailwindcss-plugin": "workspace:^"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.199",
    "@types/lodash-es": "^4.17.9",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "tailwind-config-viewer": "^1.7.2"
  }
}