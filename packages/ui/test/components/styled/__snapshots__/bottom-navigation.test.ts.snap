exports[`bottom-navigation > snap 1`] = `
".btm-nav {
  @apply bg-base-100 h-16 text-current;
}
.btm-nav > * {
  @apply border-current;
  /* disabled */
}
.btm-nav > *:not(.active) {
  @apply pt-0.5;
}
.btm-nav > *:where(.active) {
  @apply bg-base-100 border-t-2;
}
.btm-nav > *.disabled, .btm-nav > *[disabled] {
  @apply bg-neutral text-base-content pointer-events-none border-opacity-0 bg-opacity-10 text-opacity-20;
}
@media (hover: hover) {
  .btm-nav > *.disabled:hover, .btm-nav > *[disabled]:hover {
    @apply bg-neutral text-base-content pointer-events-none border-opacity-0 bg-opacity-10 text-opacity-20;
  }
}
.btm-nav > * .label {
  @apply text-base;
}"
`;
