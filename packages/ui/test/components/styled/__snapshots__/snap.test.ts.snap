exports[`rating > snap 1`] = `
".rating input {
  @apply appearance-none;
}
.rating :where(input) {
  animation: rating-pop var(--ice-animation-input, 0.25s) ease-out;
  @apply bg-base-content h-6 w-6 bg-opacity-100;
}
.rating .rating-hidden {
  @apply w-2 bg-transparent;
}
.rating input:checked ~ input,
.rating input[aria-checked=true] ~ input {
  @apply bg-opacity-20;
}
.rating input:focus-visible {
  @apply transition-transform duration-300 ease-out;
  transform: translateY(-0.125em);
}
.rating input:active:focus {
  animation: none;
}
.rating input:active:focus {
  transform: translateY(-0.125em);
}

.rating-half :where(input:not(.rating-hidden)) {
  @apply w-3;
}

@keyframes rating-pop {
  0% {
    transform: translateY(-0.125em);
  }
  40% {
    transform: translateY(-0.125em);
  }
  100% {
    transform: translateY(0);
  }
}"
`;

exports[`stack > snap 1`] = `
".stack {
  @apply place-items-center items-end;
}
.stack > * {
  @apply w-full;
}
.stack > * {
  @apply opacity-60;
}
.stack > *:nth-child(2) {
  @apply opacity-80;
}
.stack > *:nth-child(1) {
  @apply opacity-100;
}"
`;

exports[`steps > snap 1`] = `
".steps {
