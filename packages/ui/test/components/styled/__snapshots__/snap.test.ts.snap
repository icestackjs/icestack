exports[`radial-progress > snap 1`] = `
".radial-progress {
  --ice-value: 0;
  --ice-size: 5rem;
  --ice-thickness: calc(var(--ice-size) / 10);
}

.radial-progress:after {
  @apply bg-current;
}"
