  } */
}
.steps .step {
  grid-template-rows: 40px 1fr;
  grid-template-columns: auto;
  min-width: 4rem;
}
.steps .step:before {
  @apply bg-base-300 text-base-content top-0 col-start-1 row-start-1 h-2 w-full transform;
  content: \\"\\";
  margin-left: -100%;
}
.steps .step:after {
  content: counter(step);
  counter-increment: step;
  z-index: 1;
  @apply bg-base-300 text-base-content relative col-start-1 row-start-1 grid h-8 w-8 place-items-center place-self-center rounded-full;
}
.steps .step:first-child:before {
  content: none;
}
.steps .step[data-content]:after {
  content: attr(data-content);
}
.steps .step-neutral + .step-neutral:before,
.steps .step-neutral:after {
  @apply bg-neutral text-neutral-content;
}
.steps .step-primary + .step-primary:before,
.steps .step-primary:after {
  @apply bg-primary text-primary-content;
}
.steps .step-info + .step-info:before {
  @apply bg-info;
}
.steps .step-info:after {
  @apply bg-info text-info-content;
}
.steps .step-success + .step-success:before {
  @apply bg-success;
}
.steps .step-success:after {
  @apply bg-success text-success-content;
}
.steps .step-warning + .step-warning:before {
  @apply bg-warning;
}
.steps .step-warning:after {
  @apply bg-warning text-warning-content;
}
.steps .step-error + .step-error:before {
  @apply bg-error;
}
.steps .step-error:after {
  @apply bg-error text-error-content;
}"
`;

exports[`toast > snap 1`] = `
".toast {
  @apply gap-2 p-4;
}
.toast > * {
  animation: toast-pop 0.25s ease-out;
}

@keyframes toast-pop {
  0% {
    transform: scale(0.9);
    opacity: 0;
  }
  100% {
    transform: scale(1);
    opacity: 1;
  }
}"
`;
