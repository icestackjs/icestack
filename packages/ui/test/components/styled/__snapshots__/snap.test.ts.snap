  border-width: 0 0 var(--ice-tab-border, 1px) 0;
  border-top-left-radius: var(--ice-tab-radius, 0.5rem);
  border-top-right-radius: var(--ice-tab-radius, 0.5rem);
  border-bottom-color: var(--ice-tab-border-color);
  padding-left: var(--ice-tab-padding, 1rem);
  padding-right: var(--ice-tab-padding, 1rem);
  padding-top: var(--ice-tab-border, 1px);
}
.tab-lifted.tab-active:not(.tab-disabled):not([disabled]) {
  background-color: var(--ice-tab-bg);
  border-width: var(--ice-tab-border, 1px) var(--ice-tab-border, 1px) 0 var(--ice-tab-border, 1px);
  border-left-color: var(--ice-tab-border-color);
  border-right-color: var(--ice-tab-border-color);
