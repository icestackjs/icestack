{
  "name": "@icestack/unocss",
  "version": "1.3.8",
  "description": "The Unocss plugin of @icestack/ui",
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "homepage": "https://ui.icebreaker.top",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonofmagic/icestack"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/icestack"
  },
  "keywords": [
    "unocss",
    "plugin",
    "css",
    "component",
    "generate",
    "generator",
    "tailwindcss",
    "icestack",
    "ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:ui && unbuild",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "build:ui": "icestack build",
    "sync": "cnpm sync @icestack/unocss"
  },
  "dependencies": {
    "@icestack/logger": "workspace:^",
    "@icestack/shared": "workspace:^",
    "postcss": "^8.4.47",
    "postcss-js": "^4.0.1",
    "postcss-selector-parser": "^6.1.1"
  },
  "devDependencies": {
    "@icestack/types": "workspace:^",
    "@icestack/ui": "workspace:^",
    "@unocss/core": "^0.63.4",
    "unocss": "^0.63.4"
  }
}
