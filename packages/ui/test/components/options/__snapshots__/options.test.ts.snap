exports[`'menu' options > raw snap 1`] = `
{
  "defaults": {
    "base": {
      ".menu": {
        "& li.disabled": {
          "apply": [
            "cursor-not-allowed select-none",
          ],
        },
        ":where(li ul)": {
          "apply": [
            "relative whitespace-nowrap",
          ],
        },
        ":where(li:not(.menu-title)>*:not(ul):not(details):not(.menu-title)),:where(li:not(.menu-title)>details>summary:not(.menu-title))": {
          "apply": [
            "grid grid-flow-col content-start items-center gap-2",
          ],
          "css": {
            "grid-auto-columns": "minmax(auto, max-content) auto max-content",
            "user-select": "none",
          },
        },
        ":where(li>.menu-dropdown:not(.menu-dropdown-show))": {
          "apply": [
            "hidden",
          ],
        },
        "apply": [
          "flex flex-col flex-wrap text-sm",
        ],
      },
      ":where(.menu li)": {
        ".badge": {
          "apply": [
            "justify-self-end",
          ],
        },
        "apply": [
          "relative flex shrink-0 flex-col flex-wrap items-stretch",
        ],
      },
    },
    "styled": {
      ".menu": {
        "apply": [
          "p-2",
        ],
      },
      ".menu :where(li ul)": {
        "&:before": {
          "apply": [
            "bg-base-content absolute bottom-3 start-0 top-3 w-px opacity-10",
          ],
          "css": {
            "content": """",
          },
        },
        "apply": [
          "ms-4 ps-2",
        ],
      },
      ".menu :where(li:not(.menu-title)>*:not(ul):not(details):not(.menu-title)),.menu :where(li:not(.menu-title)>details>summary:not(.menu-title))": {
        "apply": [
          "rounded-btn px-4 py-2 text-start transition duration-200 ease-out",
        ],
        "css": {
          "text-wrap": "balance",
        },
      },
      ".menu :where(li>details>summary)::-webkit-details-marker": {
        "apply": [
          "hidden",
        ],
      },
      ".menu :where(li>details>summary):after,.menu :where(li>.menu-dropdown-toggle):after": {
        "apply": [
          "justify-self-end",
        ],
        "css": {
          "box-shadow": "2px 2px",
          "content": """",
          "display": "block",
          "height": "0.5rem",
          "margin-top": "-0.5rem",
          "pointer-events": "none",
          "transform": "rotate(45deg)",
          "transform-origin": "75% 75%",
          "transition-duration": "0.3s",
          "transition-property": "transform, margin-top",
          "transition-timing-function": "cubic-bezier(0.4, 0, 0.2, 1)",
          "width": "0.5rem",
        },
      },
      ".menu :where(li>details[open]>summary):after,.menu :where(li>.menu-dropdown-toggle.menu-dropdown-show):after": {
        "css": {
          "margin-top": "0",
          "transform": "rotate(225deg)",
        },
      },
      ".menu li.disabled": {
        "apply": [
          "text-base-content/30",
        ],
      },
      ".menu li>*:not(ul):not(.menu-title):not(details):active,.menu li>*:not(ul):not(.menu-title):not(details).active,.menu li>details>summary:active": {
        "apply": [
          "bg-neutral text-neutral-content [@media(hover:hover)]:bg-neutral [@media(hover:hover)]:text-neutral-content",
        ],
      },
      ".menu-title": {
        "apply": [
          "text-base-content/40 px-4 py-2 text-sm font-bold",
        ],
      },
