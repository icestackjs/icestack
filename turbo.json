{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "test:dev": {
      "dependsOn": [
        "^build"
      ]
    },
    "release": {},
    "sync": {}
  }
}