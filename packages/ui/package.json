{
  "name": "@icestack/ui",
  "version": "0.2.0",
  "description": "❤️ IceStack, Web UI for Mobile, PC and Mini Program, open-source Headless component library generator",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./defaults": {
      "types": "./dist/defaults.d.ts",
      "import": "./dist/defaults.mjs",
      "require": "./dist/defaults.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.mjs",
      "require": "./dist/components.cjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "scss"
  ],
  "bin": {
    "icestack": "bin/icestack.js"
  },
  "scripts": {
    "build": "npm run build:all && npm run static && npm run build:plugin",
    "build:all": "ts-node --esm scripts/build-scss.ts",
    "dev": "npm run clean && npm run build:all && unbuild --sourcemap",
    "bp": "unbuild",
    "build:plugin": "unbuild",
    "clean": "node scripts/clean.js",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:dev": "vitest",
    "build:base": "ts-node --esm scripts/build-scss.ts base",
    "build:utilities": "ts-node --esm scripts/build-scss.ts utilities",
    "build:components": "ts-node --esm scripts/build-scss.ts components",
    "get-table": "node scripts/get-table.js",
    "get-group": "node scripts/get-group.js",
    "get-svg": "node scripts/svg64.js",
    "static": "npm run get-table && npm run get-group && npm run get-svg",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "raw": "node bin/icestack.js inspect -o ./all0.json",
    "sync": "cnpm sync @icestack/ui"
  },
  "dependencies": {
    "@babel/generator": "^7.23.5",
    "@babel/types": "^7.23.5",
    "@ctrl/tinycolor": "^4.0.2",
    "@icestack/logger": "workspace:^",
    "@icestack/postcss": "workspace:^",
    "@icestack/shared": "workspace:^",
    "@icestack/types": "workspace:^",
    "c12": "^1.5.1",
    "commander": "^11.1.0",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@icestack/presets": "workspace:^",
    "@types/css-selector-tokenizer": "^0.7.4",
    "@types/deasync": "^0.1.5",
    "@types/fibers": "^3.1.4",
    "@types/findup-sync": "^4.0.4",
    "@types/flat-cache": "^2.0.2",
    "@types/klaw-sync": "^6.0.5",
    "@types/lodash": "^4.14.202",
    "@types/object-hash": "^3.0.6",
    "@types/rtlcss": "^3.5.4",
    "@types/serialize-javascript": "^5.0.4",
    "compose-tailwindcss-plugins": "workspace:^",
    "css-to-tailwindcss-plugin": "workspace:^",
    "del": "^7.1.0",
    "fast-glob": "^3.3.2",
    "findup-sync": "^5.0.0",
    "flat-cache": "^3.2.0",
    "klaw-sync": "^6.0.0",
    "lru-cache": "^10.1.0",
    "merge": "^2.1.1",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.31",
    "rtlcss": "^4.1.1",
    "sass-true": "^7.0.0",
    "serialize-javascript": "^6.0.1",
    "svg64": "^2.0.0",
    "tailwindcss": "3.3.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/icestack"
  },
  "homepage": "https://ui.icebreaker.top",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonofmagic/icestack"
  },
  "keywords": [
    "ui",
    "extend",
    "css",
    "generator",
    "tailwindcss",
    "unocss",
    "components",
    "ui-library",
    "postcss",
    "build"
  ]
}
