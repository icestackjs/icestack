{
  "name": "pnpm-turbo-monorepo-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-icebreaker": "^1.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "tailwind-css-variables-theme-generator": "^0.4.0",
    "tailwindcss": "^3.3.3",
    "turbo": "^1.10.9",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.6.7"
}