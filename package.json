{
  "name": "pnpm-turbo-monorepo-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build -F=./packages/*",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run"
  },
  "devDependencies": {
    "@babel/generator": "^7.22.9",
    "@babel/parser": "^7.22.7",
    "@babel/traverse": "^7.22.8",
    "@babel/types": "^7.22.5",
    "@csstools/postcss-cascade-layers": "^4.0.0",
    "@linaria/babel-preset": "^4.5.4",
    "@linaria/core": "^4.5.4",
    "@linaria/react": "^4.5.4",
    "@linaria/vite": "^4.5.4",
    "@linaria/webpack-loader": "^4.5.4",
    "@pandacss/config": "^0.8.0",
    "@pandacss/dev": "^0.8.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.20.1",
    "@types/klaw": "^3.0.3",
    "@types/postcss-js": "^4.0.1",
    "@vitest/coverage-v8": "^0.33.0",
    "chokidar": "^3.5.3",
    "clsx": "^2.0.0",
    "defu": "^6.1.2",
    "eslint": "^8.46.0",
    "eslint-config-icebreaker": "^1.2.2",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "klaw": "^4.1.0",
    "polished": "^4.2.2",
    "postcss": "^8.4.27",
    "postcss-js": "^4.0.1",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "tailwind-css-variables-theme-generator": "^0.4.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.33.0",
    "weapp-pandacss": "^1.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.6.7"
