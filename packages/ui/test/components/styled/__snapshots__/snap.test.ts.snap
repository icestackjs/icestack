  position: absolute;
  width: var(--ice-tab-radius, 0.5rem);
  height: var(--ice-tab-radius, 0.5rem);
  bottom: 0;
  --ice-tab-grad: calc(68% - var(--ice-tab-border, 1px));
  --ice-tab-corner-bg: radial-gradient(
    circle at var(--ice-circle-pos),
    transparent var(--ice-tab-grad),
    var(--ice-tab-border-color) calc(var(--ice-tab-grad) + 0.3px),
    var(--ice-tab-border-color) calc(var(--ice-tab-grad) + var(--ice-tab-border, 1px)),
    var(--ice-tab-bg) calc(var(--ice-tab-grad) + var(--ice-tab-border, 1px) + 0.3px)
  );
}
.tab-lifted.tab-active:not(.tab-disabled):not([disabled]):before {
  left: calc(var(--ice-tab-radius, 0.5rem) * -1);
