{
  "name": "@icestack/postcss-utils",
  "description": "The postcss utils of @icestack/ui",
  "version": "1.3.8",
  "scripts": {
    "build": "unbuild",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "sync": "cnpm sync @icestack/postcss-utils"
  },
  "keywords": [
    "postcss",
    "processor",
    "css",
    "component",
    "generate",
    "generator",
    "tailwindcss",
    "unocss",
    "icestack",
    "ui"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./ast": {
      "types": "./dist/ast.d.ts",
      "import": "./dist/ast.mjs",
      "require": "./dist/ast.cjs"
    },
    "./js": {
      "types": "./dist/js.d.ts",
      "import": "./dist/js.mjs",
      "require": "./dist/js.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins.d.ts",
      "import": "./dist/plugins.mjs",
      "require": "./dist/plugins.cjs"
    },
    "./scss": {
      "types": "./dist/scss.d.ts",
      "import": "./dist/scss.mjs",
      "require": "./dist/scss.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@icestack/shared": "workspace:^",
    "@icestack/types": "workspace:^",
    "css-selector-tokenizer": "^0.8.0",
    "defu": "^6.1.4",
    "lodash": "^4.17.21",
    "postcss": "^8.4.41",
    "postcss-custom-property-prefixer": "workspace:^",
    "postcss-cva": "workspace:^",
    "postcss-js": "^4.0.1",
    "postcss-scss": "^4.0.9",
    "postcss-selector-parser": "^6.1.1",
    "postcss-value-parser": "^4.2.0",
    "tailwindcss": "^3.4.9"
  },
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/icestack"
  },
  "homepage": "https://ui.icebreaker.top",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonofmagic/icestack"
  }
}