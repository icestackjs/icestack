{
  "name": "icestack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build -F=./packages/*",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test --filter='./packages/*'",
    "test2": "vitest run",
    "release": "turbo run release",
    "publish-packages": "turbo run build test --filter='./packages/*' && changeset version && changeset publish",
    "preinstall": "npx only-allow pnpm",
    "sync": "cnpm sync css-to-tailwindcss-plugin compose-tailwindcss-plugins"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.2",
    "@babel/types": "^7.23.0",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@csstools/postcss-cascade-layers": "^4.0.0",
    "@linaria/babel-preset": "^5.0.3",
    "@linaria/core": "^5.0.2",
    "@linaria/react": "^5.0.3",
    "@linaria/vite": "^5.0.3",
    "@linaria/webpack-loader": "^5.0.3",
    "@pandacss/config": "^0.15.5",
    "@pandacss/dev": "^0.15.5",
    "@types/babel__generator": "^7.6.5",
    "@types/babel__traverse": "^7.20.2",
    "@types/klaw": "^3.0.4",
    "@types/lodash": "^4.14.199",
    "@types/postcss-js": "^4.0.2",
    "@vitest/coverage-v8": "^0.34.6",
    "chokidar": "^3.5.3",
    "clsx": "^2.0.0",
    "dedent": "^1.5.1",
    "defu": "^6.1.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-icebreaker": "^1.2.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "klaw": "^4.1.0",
    "lodash": "^4.17.21",
    "only-allow": "^1.2.1",
    "polished": "^4.2.2",
    "postcss": "^8.4.31",
    "postcss-js": "^4.0.1",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "tailwind-config-viewer": "^1.7.2",
    "tailwind-css-variables-theme-generator": "^0.4.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "turbo": "^1.10.15",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6",
    "weapp-pandacss": "^1.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.6.7"
}