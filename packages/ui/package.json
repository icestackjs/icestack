{
  "name": "@icestack/ui",
  "version": "0.0.0-alpha.6",
  "description": "❤️ IceStack, Web UI for Mobile and PC , open-source Headless component library generator",
  "license": "MIT",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./tailwindcss": {
      "types": "./dist/tailwindcss.d.ts",
      "import": "./dist/tailwindcss.mjs",
      "require": "./dist/tailwindcss.cjs"
    },
    "./defaults": {
      "types": "./dist/defaults.d.ts",
      "import": "./dist/defaults.mjs",
      "require": "./dist/defaults.cjs"
    },
    "./css/*": "./assets/css/*",
    "./css-resolved/*": "./assets/css-resolved/*",
    "./js/*": "./assets/js/*",
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.cjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.cjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "assets"
  ],
  "bin": {
    "icestack": "bin/icestack.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:all && npm run build:plugin",
    "build:all": "ts-node --esm scripts/build-scss.ts",
    "build:plugin": "unbuild --sourcemap",
    "_build:plugin": "unbuild",
    "clean": "node scripts/clean.js",
    "dev": "unbuild --watch",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:dev": "vitest",
    "build:base": "ts-node --esm scripts/build-scss.ts base",
    "build:utilities": "ts-node --esm scripts/build-scss.ts utilities",
    "build:components": "ts-node --esm scripts/build-scss.ts components",
    "get-coms": "ts-node --esm scripts/get-coms.ts",
    "get-table": "node scripts/get-table.js",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "raw": "node bin/icestack.js cache clean"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@babel/generator": "^7.23.3",
    "@babel/types": "^7.23.3",
    "@ctrl/tinycolor": "^4.0.2",
    "c12": "^1.5.1",
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "css-selector-tokenizer": "^0.8.0",
    "findup-sync": "^5.0.0",
    "immutable": "^4.0.0",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.31",
    "postcss-custom-property-prefixer": "workspace:^",
    "postcss-js": "^4.0.1",
    "postcss-selector-parser": "^6.0.13",
    "rtlcss": "^4.1.1",
    "sass": "^1.69.5",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@types/css-selector-tokenizer": "^0.7.4",
    "@types/deasync": "^0.1.5",
    "@types/fibers": "^3.1.4",
    "@types/findup-sync": "^4.0.4",
    "@types/flat-cache": "^2.0.2",
    "@types/klaw-sync": "^6.0.4",
    "@types/lodash": "^4.14.201",
    "@types/object-hash": "^3.0.6",
    "@types/rtlcss": "^3.5.4",
    "@types/serialize-javascript": "^5.0.4",
    "compose-tailwindcss-plugins": "workspace:^",
    "css-to-tailwindcss-plugin": "workspace:^",
    "del": "^7.1.0",
    "fast-glob": "^3.3.2",
    "flat-cache": "^3.1.1",
    "lru-cache": "^10.0.2",
    "merge": "^2.1.1",
    "object-hash": "^3.0.0",
    "sass-true": "^7.0.0",
    "serialize-javascript": "^6.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}