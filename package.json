{
  "name": "icestack",
  "version": "0.1.9",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=./packages/*",
    "build:plugins": "turbo run build --filter=./plugins/*",
    "build:all": "pnpm build:plugins && pnpm build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test --filter='./packages/*'",
    "test2": "vitest run",
    "release": "turbo run release",
    "publish-packages": "pnpm build:all && pnpm test && changeset version && changeset publish",
    "preinstall": "npx only-allow pnpm",
    "sync": "turbo run sync",
    "bump": "bumpp -r",
    "cpmd": "node scripts/cp-md.js"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/babel__generator": "^7.6.7",
    "@types/babel__traverse": "^7.20.4",
    "@types/klaw": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/postcss-js": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^0.34.6",
    "bumpp": "^9.2.0",
    "ci-info": "^4.0.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.1",
    "defu": "^6.1.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.54.0",
    "eslint-config-icebreaker": "^1.2.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "klaw": "^4.1.0",
    "lodash": "^4.17.21",
    "only-allow": "^1.2.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "tailwind-config-viewer": "^1.7.3",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.9.0"
}