// Vitest Snapshot v1, https://vitest.dev/guide/snapshot.html

exports[`artboard > snap 1`] = `
".artboard-demo {
  @apply bg-base-300 text-base-content rounded-box;
  box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06);
}"
`;

exports[`divider > snap 1`] = `
".divider-horizontal {
  @apply mx-4 my-0 h-auto w-4;
}

.divider-vertical {
  @apply mx-0 my-4 h-4 w-auto;
}"
`;

exports[`menu > snap 1`] = `
".menu-horizontal > li:not(.menu-title) > details > ul {
  @apply ml-0 mt-4 py-2 pr-2;
}
.menu-horizontal > li > details > ul:before {
  content: none;
}

:where(.menu-horizontal > li:not(.menu-title) > details > ul) {
  @apply bg-base-100 rounded-box shadow-xl;
}

.menu-vertical > li:not(.menu-title) > details > ul {
  @apply ml-4 mt-0 py-0 pr-0;
}

.menu-xs :where(li:not(.menu-title) > *:not(ul):not(details):not(.menu-title)),
.menu-xs :where(li:not(.menu-title) > details > summary:not(.menu-title)) {
  @apply rounded px-2 py-1 text-xs;
}
.menu-xs .menu-title {
  @apply px-2 py-1;
}

.menu-sm :where(li:not(.menu-title) > *:not(ul):not(details):not(.menu-title)),
.menu-sm :where(li:not(.menu-title) > details > summary:not(.menu-title)) {
  @apply rounded-btn px-3 py-1 text-sm;
}
.menu-sm .menu-title {
  @apply px-3 py-2;
}

.menu-md :where(li:not(.menu-title) > *:not(ul):not(details):not(.menu-title)),
.menu-md :where(li:not(.menu-title) > details > summary:not(.menu-title)) {
  @apply rounded-btn px-4 py-2 text-sm;
}
.menu-md .menu-title {
  @apply px-4 py-2;
}

.menu-lg :where(li:not(.menu-title) > *:not(ul):not(details):not(.menu-title)),
.menu-lg :where(li:not(.menu-title) > details > summary:not(.menu-title)) {
  @apply rounded-btn px-6 py-3 text-lg;
}
.menu-lg .menu-title {
  @apply px-6 py-3;
}"
`;

exports[`modal > snap 1`] = `
".modal-top :where(.modal-box) {
  @apply rounded-b-box w-full max-w-none -translate-y-10 scale-100 rounded-t-none;
}

.modal-middle :where(.modal-box) {
  @apply rounded-t-box rounded-b-box w-11/12 max-w-lg translate-y-0 scale-90;
}

.modal-bottom :where(.modal-box) {
  @apply rounded-t-box w-full max-w-none translate-y-10 scale-100 rounded-b-none;
}"
`;

exports[`stat > snap 1`] = `
".stats-horizontal {
  @apply divide-x divide-y-0 overflow-x-auto;
}

.stats-vertical {
  @apply divide-x-0 divide-y overflow-y-auto;
}"
`;

exports[`steps > snap 1`] = `
".steps-horizontal .step {
  grid-template-rows: 40px 1fr;
  grid-template-columns: auto;
  min-width: 4rem;
}
.steps-horizontal .step:before {
  @apply h-2 w-full translate-x-0 translate-y-0;
  content: \\"\\";
  margin-left: -100%;
}
.steps-vertical .step {
  gap: 0.5rem;
  grid-template-columns: 40px 1fr;
  grid-template-rows: auto;
  min-height: 4rem;
  justify-items: start;
}
.steps-vertical .step:before {
  @apply h-full w-2 -translate-x-1/2 -translate-y-1/2;
  margin-left: 50%;
}
[dir=rtl] .steps-vertical .step:before {
  margin-right: auto;
}"
`;

exports[`table > snap 1`] = `
".table-xs :not(thead):not(tfoot) tr {
  @apply text-xs;
}

.table-xs :where(th, td) {
  @apply px-2 py-1;
}

.table-sm :not(thead):not(tfoot) tr {
  @apply text-sm;
}

.table-sm :where(th, td) {
  @apply px-3 py-2;
}

.table-md :not(thead):not(tfoot) tr {
  @apply text-sm;
}

.table-md :where(th, td) {
  @apply px-4 py-3;
}

.table-lg :not(thead):not(tfoot) tr {
  @apply text-base;
}

.table-lg :where(th, td) {
  @apply px-6 py-4;
}"
`;

exports[`tooltip > snap 1`] = `
".tooltip {
  @apply relative inline-block text-center;
  --ice-tooltip-tail: 0.1875rem;
  --ice-tooltip-color: hsl(var(--ice-n));
  --ice-tooltip-text-color: hsl(var(--ice-nc));
  --ice-tooltip-tail-offset: calc(100% + 0.0625rem - var(--ice-tooltip-tail));
}

.tooltip:before,
.tooltip:after {
  @apply opacity-0 transition delay-100 duration-200 ease-in-out;
}

.tooltip:after {
  @apply absolute;
}

.tooltip:before {
  @apply max-w-xs rounded px-2 py-1 text-sm;
  background-color: var(--ice-tooltip-color);
  color: var(--ice-tooltip-text-color);
  width: max-content;
}

.tooltip.tooltip-open:before,
.tooltip.tooltip-open:after,
.tooltip:hover:before,
.tooltip:hover:after {
  @apply opacity-100 delay-75;
}

.tooltip:has(:focus-visible):after,
.tooltip:has(:focus-visible):before {
  @apply opacity-100 delay-75;
}

.tooltip:not([data-tip]):hover:before,
.tooltip:not([data-tip]):hover:after {
  visibility: hidden;
  opacity: 0;
}

.tooltip:after {
  content: \\"\\";
  border-style: solid;
  border-width: var(--ice-tooltip-tail, 0);
  width: 0;
  height: 0;
  display: block;
  position: absolute;
}

.tooltip:after,
.tooltip-top:after {
  transform: translateX(-50%);
  border-color: var(--ice-tooltip-color) transparent transparent transparent;
  top: auto;
  left: 50%;
  right: auto;
  bottom: var(--ice-tooltip-tail-offset);
}

.tooltip-bottom:after {
  transform: translateX(-50%);
  border-color: transparent transparent var(--ice-tooltip-color) transparent;
  top: var(--ice-tooltip-tail-offset);
  left: 50%;
  right: auto;
  bottom: auto;
}

.tooltip-left:after {
  transform: translateY(-50%);
  border-color: transparent transparent transparent var(--ice-tooltip-color);
  top: 50%;
  left: auto;
  right: calc(var(--ice-tooltip-tail-offset) + 0.0625rem);
  bottom: auto;
}

.tooltip-right:after {
  transform: translateY(-50%);
  border-color: transparent var(--ice-tooltip-color) transparent transparent;
  top: 50%;
  left: calc(var(--ice-tooltip-tail-offset) + 0.0625rem);
  right: auto;
  bottom: auto;
}

.tooltip-primary {
  --ice-tooltip-color: hsl(var(--ice-p));
  --ice-tooltip-text-color: hsl(var(--ice-pc));
}
.tooltip-secondary {
  --ice-tooltip-color: hsl(var(--ice-s));
  --ice-tooltip-text-color: hsl(var(--ice-sc));
}
.tooltip-accent {
  --ice-tooltip-color: hsl(var(--ice-a));
  --ice-tooltip-text-color: hsl(var(--ice-ac));
}
.tooltip-info {
  --ice-tooltip-color: hsl(var(--ice-in));
  --ice-tooltip-text-color: hsl(var(--ice-inc));
}
.tooltip-success {
  --ice-tooltip-color: hsl(var(--ice-su));
  --ice-tooltip-text-color: hsl(var(--ice-suc));
}
.tooltip-warning {
  --ice-tooltip-color: hsl(var(--ice-wa));
  --ice-tooltip-text-color: hsl(var(--ice-wac));
}
.tooltip-error {
  --ice-tooltip-color: hsl(var(--ice-er));
  --ice-tooltip-text-color: hsl(var(--ice-erc));
}"
`;
