{
  "name": "postcss-cva",
  "description": "generate cva functions by css comment",
  "version": "0.2.5",
  "scripts": {
    "dev": "unbuild --sourcemap",
    "build": "unbuild",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "sync": "cnpm sync postcss-cva",
    "md": "node scripts/md.js"
  },
  "keywords": [
    "cva",
    "class-variance-authority",
    "postcss",
    "generate",
    "generator",
    "tailwindcss",
    "unocss",
    "icestack",
    "ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./extract": {
      "types": "./dist/extract.d.ts",
      "import": "./dist/extract.mjs",
      "require": "./dist/extract.cjs"
    },
    "./generator": {
      "types": "./dist/generator.d.ts",
      "import": "./dist/generator.mjs",
      "require": "./dist/generator.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@babel/generator": "^7.24.4",
    "@babel/types": "^7.24.0",
    "@icestack/shared": "workspace:^",
    "lodash": "^4.17.21",
    "picomatch": "^4.0.2",
    "postcss-selector-parser": "^6.0.16"
  },
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/icestack"
  },
  "homepage": "https://ui.icebreaker.top/docs/cva-generate",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonofmagic/icestack"
  },
  "devDependencies": {
    "@types/picomatch": "^2.3.3",
    "postcss": "^8.4.38"
  }
}